# Environment Variables Template
# Copy this file to .env and fill in your actual values

# reCAPTCHA Configuration (v3)
# Get these from https://www.google.com/recaptcha/admin
PUBLIC_RECAPTCHA_SITE_KEY=your_site_key_here
RECAPTCHA_SECRET_KEY=your_secret_key_here

# Notification Service (ntfy.sh or similar)
NTFY_URL=your_ntfy_url_here
BASIC_AUTH_USERNAME=your_username_here
BASIC_AUTH_PASSWORD=your_password_here
